#aliasy pomocnicze
alias latin_short="sleep 1800; say -v Alice Diskipulus bonus. Intermissio habet." 
alias latin_long="sleep 3000; say -v Alice Diskipulus bonus. Intermissio habet." 

# Aliases to books
