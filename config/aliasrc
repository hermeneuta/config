# Study, learning, project sessions 
alias tt="timew start Typing; gtypist; timew stop" #touch typing with gnu-typist
alias reading_short="timew start Reading; echo 'Krótka sesja czytania (30 min)'; say Short reading session started; sleep 1800; say Session over. Take a break; timew stop"
alias reading_long="timew start Reading; echo 'Długa sesja czytania (50 min)'; say Long reading session started; sleep 3000; say Session over. Take a break; timew stop"
alias study_cs_short="timew start CS; echo 'Short CS Session (30 min)'; say Short CS session started; sleep 1800; say Session over. Nice work. Have a break and consider to resume after; timew stop"
alias study_cs_long="timew start CS; echo 'Long CS Session (50 min)'; say Long CS session started; sleep 3000; say Session over. Nice work. Have a break and consider to resume after; timew stop"
alias study_scripting='timew start CS Scripting; zathura ~/Documents/Mathesis/The\ Linux\ Command\ Line.pdf &; echo "50 min session with Linux has started"; sleep 3000; say Have a handsome reward for a task well performed; timew stop'
alias study_latina_parva='timew start Latina; zathura -P 495 ~/Documents/Latin/Wheelock/wheelock.pdf &; zathura ~/Documents/Latin/beginners_latin.pdf &; echo "Short Session (30 min) Started..."; latin_short; timew stop' 
alias study_latina_longa='timew start Latina; zathura -P 495 ~/Documents/Latin/Wheelock/wheelock.pdf &; zathura ~/Documents/Latin/beginners_latin.pdf &; echo "Long Session (50 min) Started..."; latin_long; timew stop'
alias project_latina_empire="timew start C CS 'Latin Empire'; echo 'Short CS Session (30 min)'; say Short CS session started; sleep 1800; say Session over. Nice work. Have a break and consider to resume after; timew stop"
alias config="timew start Config; say Configuration started; sleep 1800; say thirty minutes of configuration have passed. Stop time warrior manually."

