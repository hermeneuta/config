#Taskwarrior
alias d="clear; task" #d jako skrót od do. Alias do taskwarrior
alias mod="task modify" #pozwala modyfikować dane zadanie. Po komendzie potrzebny identyfikator, e.g. mod 5 due:today
alias don="task done" #zakończenie zadania. Po komendzie potrzebny indentyfikator
alias add="task add" #dodawanie zadań
alias del='task delete'
alias work='clear; task +work' #lista zadań z tagiem work
alias study='clear; task +study' #lista zadań z tagiem study
alias purchase='clear; task +purchase' #lista zadań z tagiem purchase
alias cs='clear; task project:CS' #lista zadań dla projektu CS 

#timewarrior
alias t="timew" #alias dla timewarrior
alias start='timew start' #rozpoczęcie okrślonego interwału
alias stop="timew stop" #zatrzymuje aktywny interwał
alias track="timew track" #pozwala na wprowadzenie zakończonego interwału
alias day='timew day rc.reports.day.hours=auto rc.reports.day.lines=4 rc.reports.day.axis=external'
alias week='timew week'
alias summary='timew summary :ids'
alias tag='timew tag'
alias utag='timew untag'
alias leng='timew lengthen'

